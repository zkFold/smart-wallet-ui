{
  "manifest_version": 3,
  "name": "Smart Wallet Extension",
  "version": "1.0",
  "description": "A browser extension for managing your smart wallet.",
  "permissions": [
    "storage",
    "activeTab",
    "identity"
  ],
  "oauth2": {
    "client_id": "371998750978-6e9j9pogb76e53p1n63pvk8ac9pp11mc.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "openid"
    ]
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Open UI"
  },
  "host_permissions": [
    "https://wallet-prover.zkfold.io/v0/*",
    "https://wallet-api.zkfold.io/v0/*"
  ]
  
}